@model IEnumerable<Rating>

<div class="row">
    <div class="col-md-12">
        <div class="box">
            <div class="box-header"><span class="title"> Puntuaciones </span></div>
            <div class="box-content">
                <!-- find me in partials/data_tables_custom -->

                <div id="dataTables">
                    <table cellpadding="0" cellspacing="0" border="0" class="dTable responsive">
                        <thead>
                            <tr>
                                <td class="hidden"></td>
                                <th><div> Puntuación </div></th>
                                <th><div> Evaluado por</div></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var rating in Model)
                            {
                                <tr>
                                    <td class="hidden"></td>
                                    <td><div> @rating.Rate</div></td>
                                    <td><div> @rating.User.UserName </div></td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>